{
  "name": "eezydev",
  "version": "1.5.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "sanity:start": "sanity start",
    "sanity:build": "sanity build",
    "start": "next start",
    "lint": "eslint src --max-warnings=0",
    "test": "jest --maxWorkers=50%",
    "test:watch": "jest --watch --maxWorkers=25%",
    "test:ci": "jest --runInBand",
    "generate": "yarn plop --plopfile generators/plopfile.js",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "postinstall": "husky install",
    "prepare": "husky install",
    "commit": "cz"
  },
  "lint-staged": {
    "src/**/*": [
      "yarn lint --fix",
      "yarn test --findRelatedTests --bail"
    ]
  },
  "dependencies": {
    "@sanity/base": "^2.29.3",
    "@sanity/client": "^3.3.0",
    "@sanity/core": "^2.29.3",
    "@sanity/default-layout": "^2.29.3",
    "@sanity/default-login": "^2.29.3",
    "@sanity/desk-tool": "^2.29.4",
    "@sanity/image-url": "^1.0.1",
    "@sanity/vision": "^2.29.3",
    "aos": "^2.3.4",
    "eslint": "^8.8.0",
    "next": "12.0.10",
    "next-pwa": "^5.3.1",
    "prop-types": "^15.7",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-icons": "^4.3.1",
    "react-responsive": "^9.0.0-beta.6",
    "react-scroll": "^1.8.6",
    "styled-components": "5.3.3",
    "swiper": "^8.0.7"
  },
  "devDependencies": {
    "@storybook/addon-essentials": "6.4.17",
    "@storybook/builder-webpack5": "^6.4.17",
    "@storybook/manager-webpack5": "^6.4.17",
    "@storybook/react": "6.4.17",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.1.2",
    "@types/aos": "^3.0.4",
    "@types/jest": "^27.4.0",
    "@types/node": "^17.0.14",
    "@types/react": "^17.0.27",
    "@types/react-scroll": "^1.8.3",
    "@types/styled-components": "^5.1.21",
    "@typescript-eslint/eslint-plugin": "5.10.2",
    "@typescript-eslint/parser": "5.10.2",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "^8.8.0",
    "eslint-config-next": "^12.0.10",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.26.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^7.0.2",
    "jest": "^27.5.1",
    "jest-styled-components": "^7.0.5",
    "lint-staged": "^12.3.3",
    "plop": "^3.0.5",
    "prettier": "^2.7.1",
    "storybook-addon-next-router": "^3.0.8",
    "typescript": "^4.5.5",
    "webpack": "5.68.0"
  },
  "husky": {
    "hooks": {
      "prepare-commit-msg": "exec < /dev/tty && npx cz --hook || true"
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
